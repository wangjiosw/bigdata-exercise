var menu;
var $=jQuery;
$("#topnav li").hover(function() {
$(this).addClass("active");
menu = $(this).find(".downMenu");
function sDown() {
if (!menu.is(":animated")) {
menu.slideDown(0);
}
}
sDown();
},
function() {
$(this).removeClass("active");
menu = $(this).find(".downMenu");
function sUp() {
menu.slideUp(0);
}
sUp();
});
