<script type="text/javascript">
	if(/AppleWebKit.*mobile/i.test(navigator.userAgent) || /Android/i.test(navigator.userAgent)|| (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent))){

			nav_menu();//移动端导航
	}

	/*移动端导航*/
	var menuisshow = false;
	function nav_menu(){
		var mbns=$(".head_liebiao_xq16826 .btnall");
		var menus=$("#menu .closebtn");
		mbns.click(function(){
			$("#menu").css({display:"block"});
			menuisshow = true;
			setNavCss();
			$("#player").css('z-index','1');
			//stopDefault(e);
		})
		menus.click(function(){
			menuisshow = false;
			$("#menu").css({display:"none"});
			setNavCss();
		})
	}
	function setNavCss(){
		if(menuisshow){
			var wsc = $(window).height();
			var menuhh=$("#menu").find("ul").outerHeight();
			var lphhh=wsc>menuhh?wsc:menuhh;
			if(wsc>menuhh){
				var btz=(wsc-menuhh)/2
				$("#menu").find("ul").css({marginTop:btz+"px",marginBottom:btz+"px"})
			}
			$('.bg_top_h_tile').css('height',lphhh+'px');
			$('.bg_top_h_tile').css('overflow','hidden');
			$('.menu').css('height',lphhh+'px');
		}else{
			$('.bg_top_h_tile').css('height','auto')
			$('.menu').css('height','auto')
		}
	}


	var window_url=window.location.href;
	$(".wspzhangbo16827_ind01 .main .nav a").each(function(){
		var nav_url = $(this).attr('href');
		if (window_url==nav_url)
		{
			$(this).addClass('cur');
		}
	});
	$(".menu ul li a").each(function(){
		var nav_url = $(this).attr('href');
		if (window_url==nav_url)
		{
			$(this).addClass('cur');
		}
	});

</script>
					
