for (var i=0;i<obj.length ;i++ )
{
	item.push(obj[i]);
}

var curPage = 1;
var pagesize=9;   //一次加载多少条数据
var NumPages=0;                                                                         //总页数;
//显示某一页的内容	
function showLi(onepage){
	curPage=onepage;
	//该页的第一行
	var start=pagesize*(curPage-1)+1;
	if(start<=0||start>item.length)
	return;
	//装入该页内容
	var strText = "";
	for(var i=1;i<=pagesize;i++){		
		if(start<(item.length+1)){
				if (i%3==0)
				{
					strText +='<li class="last"><div class="img"><a  href="'+item[start-1].url+'"><img src="'+item[start-1].imgUrl+'"></a><a  href="'+item[start-1].url+'" class="'+item[start-1].content+'"><i></i><b>'+item[start-1].brief+'</b></a></div><p class="txt"><a  href="'+item[start-1].url+'">'+item[start-1].title+'</a></p><p class="date">'+item[start-1].date+'</p></li>';
				}else{
					strText +='<li><div class="img"><a  href="'+item[start-1].url+'"><img src="'+item[start-1].imgUrl+'"></a><a  href="'+item[start-1].url+'" class="'+item[start-1].content+'"><i></i><b>'+item[start-1].brief+'</b></a></div><p class="txt"><a  href="'+item[start-1].url+'">'+item[start-1].title+'</a></p><p class="date">'+item[start-1].date+'</p></li>';
				}
			start++;
		}	
	}
	$('#loaddata').append(strText);
}

var jiazaizhong = true;
// 绑定：上拉加载更多监听
function addPullLoaderEvent() {
	if(window.addEventListener) {
		window.addEventListener('scroll', scroll_load, false);//滚动条滚动执行addcomment方法
	} else {
		window.attachEvent('onscroll', scroll_load);//ie滚动条滚动执行addcomment方法
	}

}

function scroll_load(ev) {
	var sTop = document.body.scrollTop || document.documentElement.scrollTop;	//距离body的距离
	var dHeight = $(document).height(),		                //文档的高度
		cHeight = document.documentElement.clientHeight;	//可见内容区高度
	var footerH=$(".footer").height();
		NumPages=Math.ceil(item.length/pagesize);
	if(dHeight-sTop-footerH <= cHeight){
		if(jiazaizhong){
			curPage++;
			if(curPage <= NumPages){
				setTimeout(function(){
					showLi(curPage);
					jiazaizhong = true;
				},1000)
				jiazaizhong = false;
			}else{
				$('.jiazaimore').text('没有更多内容了');
			}
		}
	}
}
showLi(curPage);
scroll_load();
addPullLoaderEvent();

</script>
	</div>


	</div>


	
				
