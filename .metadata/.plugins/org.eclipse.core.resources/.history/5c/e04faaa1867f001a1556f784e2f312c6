<script type="text/javascript" src="http://r.img.cctvpic.com/library/content/tw/script/share.js"></script>
<SCRIPT LANGUAGE="JavaScript">
	var url = "http://news.cctv.com/law/data/index.json";//jingji.cntv.cn/jingji2013/data
	var current = 0 ; //当前展示到第几条数据
	var flag = false; //数据是否展示完
	var perSize = 14; //每次多展示14条
	var dataArray = new Array();
	var contentThree = false;
	var contentNine = false;
	var firstSize = 14;//第一次展示2条
		
	//第三个模块的内容
	
	//第九个模块的内容
	function loadNineHtml(){
		var html = "";
		html += '<div class="ecoA9805_con04" style="">';

		html += '<h3><span class="l" id="myNineTime"></span><div class="clear"></div></h3>';
		html += '<div class="title_list_box"><ul id="myNine"></ul></div>';
		html += '</div><div class="dot_line"></div>';
		contentNine = true;
		return html;
	}
	
	//加载更多数据
	function addData(){
		$("#open_box").remove();
		var html = $("#leftContent").html();
		var beginId = current;
		var endId = beginId+perSize;
		current = endId;
		if(endId > dataArray.length){
			flag = true;
			endId = dataArray.length;
		}
		for(var i=beginId;i<endId;i++){
			if(i==8){;
				html += loadNineHtml();					
			}
			html += loadHtml(dataArray[i]);
		}
		if(flag){
			html += "<div id='open_box' class='ecoA9805_con05'>没有更多数据</div>";
		}else{
			html += "<div id='open_box' class='ecoA9805_con05'><a href='javascript:addData()'>点击查看更多>></a></div>";
		}
		$("#leftContent").html(html);
		if(beginId<=8&&endId>=8){
			loadNineData();
		}
	}

	//加载第一页数据
	function addFirstData(){
		$("#open_box").remove();
		var html = $("#leftContent").html();
		var beginId = current;
		var endId = beginId+firstSize;
		current = endId;
		if(endId > dataArray.length){
			flag = true;
			endId = dataArray.length;
		}
		
		for(var i=beginId;i<endId;i++){
			if(i==8){
				html += loadNineHtml();
			}

			html += loadHtml(dataArray[i]);
		}
		if(flag){
			html += "<div id='open_box' class='ecoA9805_con05'>没有更多数据</div>";
		}else{
			html += "<div id='open_box' class='ecoA9805_con05'><a href='javascript:addData()'>点击查看更多>></a></div>";
		}
		$("#leftContent").html(html);
		if(beginId<=8&&endId>=8){
			loadNineData();
		}
	}
	
	//获取数据
	function getData(){
		$.getJSON(url,function(data){
			dataArray = data.rollData;
			addFirstData();
		})
	}
	
	//数据装载页面
	function loadHtml(obj){
		var tagArray = obj.content.split(" ");
		var html = "<div class=\"ecoA9805_con02\">";			
		if(obj.type=="ARTI"){
			//正文有两条相关
			if(obj.relatedNews1!=""&&obj.relatedNews2!=""){
				html += "<h3><span class=\"l\"><a href='"+obj.url+"' target='_blank'>"+obj.title+"</a></span><div class=\"clear\"></div></h3>";
				html += "<div class=\"text_box\">";
				html += "<p>"+obj.description+"</p>";
				html += "<div class=\"title_list_box\">";
				html += "<ul><li><a href='"+obj.relatedNewsUrl1+"' target='_blank'>"+obj.relatedNews1+"</a></li>";
				html += "<li><a href='"+obj.relatedNewsUrl2+"' target='_blank'>"+obj.relatedNews2+"</a></li></ul>";
				html += "</div><div class=\"clear\"></div>";
			//如果维护了编目并且维护了简介
			}else if(obj.catalog!=""&&obj.brief!=""){
				var imageUrlArray = obj.brief.split("|");
				html += "<h3><span class=\"l\"><a href='"+obj.catalog+"' target='_blank'>"+obj.title+"</a></span><div class=\"clear\"></div></h3>";
				html += "<div class=\"text_box\">";
				html += "<div class=\"image_list_box\"><dl>";
				html += "<dd><div class=\"image\"><a href='"+imageUrlArray[0]+"' target='_blank'><img src='"+obj.image+"' width=\"196\" height=\"112\" /></a><a href='"+imageUrlArray[0]+"' class=\"plus\" target='_blank'></a></div></dd>";
				html += "<dd><div class=\"image\"><a href='"+imageUrlArray[1]+"' target='_blank'><img src='"+obj.image1+"' width=\"196\" height=\"112\" /></a><a href='"+imageUrlArray[1]+"' class=\"plus\" target='_blank'></a></div></dd>";
				html += "<dd><div class=\"image\"><a href='"+imageUrlArray[2]+"' target='_blank'><img src='"+obj.image2+"' width=\"196\" height=\"112\" /></a><a href='"+imageUrlArray[2]+"' class=\"plus\" target='_blank'></a></div></dd>";
				html += "</dl></div><div class=\"clear\"></div>";	
			//如果维护了编目没有维护简介
			}else if(obj.catalog!=""&&obj.brief==""){
				html += "<h3><span class=\"l\"><a href='"+obj.url+"' target='_blank'>"+obj.title+"</a> <i><a href='"+obj.catalog+"' target='_blank'>专题</a></i></span><div class=\"clear\"></div></h3>";
				html += "<div class=\"text_box\">";
				if(obj.image!=""){
					html += "<div class=\"l\"><a href='"+obj.url+"' target='_blank'><img width=\"196\" height=\"112\" src='"+obj.image+"'></a><a class=\"plus\" href='"+obj.url+"' target='_blank'></a></div>";
				}
				html += "<p>"+obj.description+"</p>";
			//如果没维护编目维护了简介
			}else if(obj.catalog==""&&obj.brief!=""){
				var imageUrlArray = obj.brief.split("|");
				html += "<h3><span class=\"l\"><a href='"+obj.url+"' target='_blank'>"+obj.title+"</a></span><div class=\"clear\"></div></h3>";
				html += "<div class=\"text_box\">";
				html += "<div class=\"image_list_box\"><dl>";
				html += "<dd><div class=\"image\"><a href='"+imageUrlArray[0]+"' target='_blank'><img src='"+obj.image+"' width=\"196\" height=\"112\" /></a><a href='"+imageUrlArray[0]+"' class=\"plus\" target='_blank'></a></div></dd>";
				html += "<dd><div class=\"image\"><a href='"+imageUrlArray[1]+"' target='_blank'><img src='"+obj.image1+"' width=\"196\" height=\"112\" /></a><a href='"+imageUrlArray[1]+"' class=\"plus\" target='_blank'></a></div></dd>";
				html += "<dd><div class=\"image\"><a href='"+imageUrlArray[2]+"' target='_blank'><img src='"+obj.image2+"' width=\"196\" height=\"112\" /></a><a href='"+imageUrlArray[2]+"' class=\"plus\" target='_blank'></a></div></dd>";
				html += "</dl></div><div class=\"clear\"></div>";
			}else if(obj.image!=""){
				html += "<h3><span class=\"l\"><a href='"+obj.url+"' target='_blank'>"+obj.title+"</a></span><div class=\"clear\"></div></h3>";
				html += "<div class=\"text_box\">";
				html += "<div class=\"l\"><a href='"+obj.url+"' target='_blank'><img src='"+obj.image+"' width=\"196\" height=\"112\" /></a></div>";
				html += "<p>"+obj.description+"</p>";
			}else{
				html += "<h3><span class=\"l\"><a href='"+obj.url+"' target='_blank'>"+obj.title+"</a></span><div class=\"clear\"></div></h3>";
				html += "<div class=\"text_box\">";
				html += "<p>"+obj.description+"</p>";
			}			
		}else if(obj.type=="PHOA"){
			html += "<h3><span class=\"l\"><a href='"+obj.url+"' target='_blank'>"+obj.title+"</a></span><div class=\"clear\"></div></h3>";
			html += "<div class=\"text_box\">";
			if(obj.image!=""){
				html += "<div class=\"\"><a href='"+obj.url+"' target='_blank'><img src='"+obj.image+"' width=\"660\" height=\"230\" /></a></div>";
			}
	
		}else if(obj.type=="VIDE"){
			html += "<h3><span class=\"l\"><a href='"+obj.url+"' target='_blank'>"+obj.title+"</a></span><div class=\"clear\"></div></h3>";
			html += "<div class=\"text_box\">";
			html += "<div class=\"l\"><a href='"+obj.url+"' target='_blank'><img width=\"196\" height=\"112\" src='"+obj.image+"'></a><a class=\"plus\" href='"+obj.url+"' target='_blank'></a></div>";
			html += "<p>"+obj.description+"</p>";
		}
		html += "<h4>";
		for(var i=0;i<tagArray.length;i++){
			html += "<a href='http://search.cntv.cn/netall/index.shtml?qtext="+escape(tagArray[i])+"' target='_blank'>"+tagArray[i]+"</a> ";
		}
		html += "</h4>";
		html += "<h5><i>"+obj.dateTime+"</i></h5>";
		html += "</div></div><div class=\"dot_line\"></div>";
		return html;		
	}
	
	//获取新数据
	function getNewData(){
		$.getJSON(url+"?r="+Math.random(),function(data){
			var newDataArray = data.rollData;
			var distance = 0;
			if(dataArray!=null&&dataArray[0].title!=""){
				for(var i=0;i<newDataArray.length;i++){
					if(newDataArray[i].title==dataArray[0].title){
						break;
					}else{
						distance ++;
					}
				}
			}
			var html = "";
			for(var i=0;i<distance;i++){
				html += loadHtml(newDataArray[i]);
			}
			html += $("#leftContent").html();
			$("#leftContent").html(html);
			
			var topHtml = "<a href=\"javascript:getNewData()\">更新0条新闻，点击刷新</a> <a href=\"javascript:getNewData()\" class=\"shuaxin\"></a>";
			$(".moption").html(topHtml);
			dataArray =  newDataArray;
		})
	}
	
	//获取新数据的条数
	function getNewDataNum(){
		$.getJSON(url+"?r="+Math.random(),function(data){
			var newDataArray = data.rollData;
			var distance = 0;
			if(dataArray!=null&&dataArray[0].title!=""){
				for(var i=0;i<newDataArray.length;i++){
					if(newDataArray[i].title==dataArray[0].title){
						break;
					}else{
						distance ++;
					}
				}
			}
			var topHtml = "<a href=\"javascript:getNewData()\">更新"+distance+"条新闻，点击刷新</a> <a href=\"javascript:getNewData()\" class=\"shuaxin\"></a>";
			$(".moption").html(topHtml);
		})
	}

	getData();

	window.setInterval("getNewDataNum()",120000);
	
	function jsonpcallback(data){
		var myNineTime = '<i><b>各网头条汇</b></i><span >'+ data['date']+'</span>';
		var myNineHtml = "";
		var myNine = data['news_list'];
		for(var i=0;i<myNine.length;i++){
			myNineHtml += "<li><a href='"+myNine[i].link+"' target='_blank'>"+myNine[i].source+"："+myNine[i].title+"</a></li>";
		}
		$("#myNineTime").html(myNineTime);
		$("#myThreeTime").html(data['date']);
		$("#myNine").html(myNineHtml);
	}

	function loadNineData(){
		function loadjquery(m){		
			var d=document.createElement("script");
			d.setAttribute("charset","utf-8");
			d.type="text/javascript";
			d.language="javascript";
			d.src=m;
			document.getElementsByTagName("body")[0].appendChild(d);
			if(d.readyState){
			d.onreadystatechange=function(){
				if(!this.readyState||this.readyState=="loaded"||this.readyState=="complete"){
					//外部js加载完成
					delete d;
				}
			}
			}else{
			d.onload=function(){
					delete d;
			}
			}
		}
		var nineUrl="http://news.cntv.cn/xinwentoutiao/topten/EconomicHeadlines.js";
		loadjquery(nineUrl);
	}
</SCRIPT>
